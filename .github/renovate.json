{
  "extends": ["config:base", "docker:disable"],
  "automerge": true,
  "major": {
    "automerge": false
  },
  "masterIssue": true,
  "baseBranches": ["latest"],
  "additionalReviewers": ["ruheni"],
  "packageRules": [
    {
      "baseBranchList": ["latest"],
      "packageNames": ["prisma", "@prisma/client"],
