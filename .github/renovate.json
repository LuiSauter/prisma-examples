{
  "extends": ["config:base", "docker:disable"],
  "automerge": true,
  "major": {
    "automerge": false
  },
  "masterIssue": true,
  "baseBranches": ["latest"],
  "additionalReviewers": ["ruheni"],
  "packageRules": [
    {
      "matchPackagePatterns": ["@prisma/*"],
      "matchPackageNames": ["prisma"],
      "matchUpdateTypes": ["major", "minor", "patch"],
      "groupName": "Prisma Dependencies",
      "groupSlug": "prisma-deps",
    }
  ]
}
